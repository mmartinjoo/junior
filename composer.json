{
    "name": "mmartinjoo/junior-artisan",
    "description": "Testing",
    "keywords": [
        "Martin Joo",
        "laravel",
        "junior-artisan"
    ],
    "homepage": "https://github.com/mmartinjoo/junior-artisan",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Joo",
            "email": "martin@martinjoo.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.5",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Mmartinjoo\\JuniorArtisan\\": "src/",
            "Mmartinjoo\\JuniorArtisan\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmartinjoo\\JuniorArtisan\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-junior-artisan --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mmartinjoo\\JuniorArtisan\\JuniorArtisanServiceProvider"
            ],
            "aliases": {
                "JuniorArtisan": "Mmartinjoo\\JuniorArtisan\\Facades\\JuniorArtisan"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}