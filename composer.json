{
    "name": "mmartinjoo/junior",
    "version": "0.1.4",
    "description": "Junior is a Laravel artisan command that writes code for you",
    "keywords": [
        "Martin Joo",
        "laravel",
        "junior"
    ],
    "homepage": "https://github.com/mmartinjoo/junior",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Joo",
            "email": "martin@martinjoo.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8"
    },
    "autoload": {
        "psr-4": {
            "Mmartinjoo\\Junior\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmartinjoo\\Junior\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mmartinjoo\\Junior\\JuniorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
